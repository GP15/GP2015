<table class="table table-condensed">
  <thead>
    <tr>
      <th>Children</th>
      <th><%= link_to "+ add", new_user_child_path(current_user), class: "pull-right green-text" %></th>
    </tr>
  </thead>
  <tbody class="text-13px link-underline">
    <% @children.each do |child| %>
      <tr>
        <td><%= link_to child.first_name, child_path(child) %></td>
        <td class="pull-right">
          <%= age(child) %>
          <%= link_to edit_user_child_path(current_user, child) do %>
            Edit
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

