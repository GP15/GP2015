<% provide(:title, "Schedules") %>
<% provide(:nav_color, "link-white") %>

<%= render partial: 'layouts/page_title', locals: { page_title: 'Schedules' } %>
<%= render 'filters' %>

<section class="padding-one">
  <div class="container">
    <div class="row">
      <div class="col-md-12">

        <% if @schedules.present? %>
          <%= render 'schedules' %>
        <% else %>
          <p>No activity found.</p>
        <% end %>

      </div>
    </div>
  </div>
</section>
