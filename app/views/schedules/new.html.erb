<% provide(:title, 'Add schedule') %>

<section class="padding-one">
  <div class="container">
    <div class="row">
      <div class="col-md-4 center-col">

        <h1 class="title-large text-center margin-four-bottom">Add Schedule</h1>

        <%= form_for [@partner, @schedule] do |f| %>
          <%= render 'form', f: f %>

          <div class="margin-five pull-right">
            <%= link_to "Cancel", admin_partner_path(@partner), class: secondary_button_small %>
            <%= f.submit "Create", class: primary_button_small %>
          </div>
        <% end %>

      </div>
    </div>
  </div>
</section>
