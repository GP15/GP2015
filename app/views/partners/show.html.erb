<% provide(:title, @partner.company) %>

<p id="notice"><%= notice %></p>

<p>
  <strong>Company:</strong>
  <%= @partner.company %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @partner.phone %>
</p>

<p>
  <strong>Address:</strong>
  <%= @partner.address %>
</p>

<p>
  <strong>City:</strong>
  <%= @city.name unless @city.nil? %>
</p>

<p>
  <strong>State:</strong>
  <%= @partner.state %>
</p>

<p>
  <strong>Photo:</strong><br>
  <%= image_tag @partner.img_url, size: "150" %>
</p>
