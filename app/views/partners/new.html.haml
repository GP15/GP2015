- provide(:title, 'Add partner')
%section.padding-one
  .container
    .row
      .col-md-6.col-md-offset-3
        %h1.title-large.text-center.margin-four-bottom Add Partner
        = form_for @partner do |f|
          = render 'form', f: f
          .margin-five.pull-right
            = link_to "Cancel", admin_partners_path, class: secondary_button_small
            = f.submit "Create", class: primary_button_small
