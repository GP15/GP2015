- provide(:title, 'Add class')
%section.padding-one
  .container
    .row
      .col-md-6.col-md-offset-3
        %h1.title-large.text-center.margin-four-bottom Add Class
        = form_for [@partner, @klass] do |f|
          = render 'form', f: f
          .margin-five.pull-right
            = link_to "Cancel", admin_partner_klasses_path(@partner), class: secondary_button_small
            = f.submit "Create", class: primary_button_small
