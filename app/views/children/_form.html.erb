<%= render 'shared/error_messages', object: f.object %>

<div class="field">
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</div>
<div class="field">
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</div>
<div class="field">
  <%= f.label :birth_year, class: "display-block" %>
  <%= f.select :birth_year, (Date.current.year - 17)..(Date.current.year - 4) %>
</div>
